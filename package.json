{
  "name": "teleflex-pizza",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"node ./bin/www\" \"cd client && npm start\"",
    "dev": "concurrently \"nodemon ./bin/www\" \"cd client && npm start\"",
    "install-all": "concurrently \"npm install\" \"cd client && npm install\"",
    "create-tables": "knex migrate:latest",
    "migrate": "knex migrate:latest && knex seed:run",
    "reseed": "knex seed:run"
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "knex": "^0.21.5",
    "morgan": "~1.9.1",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.4"
  }
}
